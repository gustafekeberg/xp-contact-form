<?xml version="1.0" encoding="UTF-8"?>
<part>
	<display-name>Contact Form: Display form</display-name>
	<config>
		<field-set name="Config">
			<label>Config</label>
			<items>
				<input type="TextLine" name="title">
					<label>Title</label>
					<occurrences minimum="0" maximum="1"/>
				</input>
				<input type="ContentSelector" name="selectedForm">
					<label>Select form</label>
					<occurrences minimum="1" maximum="1"/>
					<config>
						<allowType>contact-form</allowType>
					</config>
				</input>
			</items>
		</field-set>
	</config>
</part>
