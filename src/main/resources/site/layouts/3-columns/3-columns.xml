<?xml version="1.0" encoding="UTF-8"?>
<layout>
	<display-name>Contact Form Layout: 3 columns</display-name>
	<config>
		<item-set name="legends">
			<label>Legends</label>
			<occurrences minimum="3" maximum="3" />
			<items>
				<input type="TextLine" name="text">
					<label>Legend</label>
					<occurrences minimum="0" maximum="1" />
				</input>
			</items>
		</item-set>
	</config>
	<regions>
		<region name="column_1"/>
		<region name="column_2"/>
		<region name="column_3"/>
	</regions>
</layout>
